<table>
  <thead>
    <th>配送先編集</th>
  </thead>
  <tbody>
    <%= form_with model: @address, url: address_path, local: true do |f| %>
    <tr>
     <td>
       <%= f.label :postal_code, "郵便番号(ハイフンなし)" %>
       <%= f.text_field :postal_code %>
     </td>
    </tr>
    <tr>
      <td>
        <%= f.label :address, "住所" %>
        <%= f.text_field :address %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label :name, "宛名" %>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存", class: "btn btn-success" %>
      </td>
    </tr>
    <% end %>
  </tbody>
