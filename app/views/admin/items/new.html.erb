<h1>商品新規登録</h1>

<%= form_with model: @item,url: admin_items_path, local:true do |f| %>
  <h4>商品画像</h4>
  <%= f.attachment_field :image %>
  <h4>商品名</h4>
  <%= f.text_field :name %>
  <h4>商品説明</h4>
  <%= f.text_area :introduction %>
  <h4>税別価格</h4>
  <%= f.text_field :price %>円<br>
  <%= f.submit '新規投稿' %>
<% end %>