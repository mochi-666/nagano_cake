<h4>会員一覧</h4>
<table>
  <tr>
    <td>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
    </td>
  </tr>
</table>
<table>
  <% @customers.each do |customer| %>
  <tr>
    <td><%= customer.id %></td>
    <td>
      <%= link_to admin_customer_path(customer.id) do %>
      <%= customer.first_name + customer.last_name %>
      <% end %>
    </td>
    <td><%= customer.email %></td>
  </tr>
  <% end %>
  <td><%= paginate @customers %></td>
  
</table>